drop table IF EXISTS DA_proximity_switch;
create table DA_proximity_switch
(
    vin                 varchar(20) not null comment 'vin',
    day                 date        not null comment '日期',
    count_input_signal0 int         null comment '输出信号0次数',
    count_input_signal1 int         null comment '输出信号1次数',
    count_input_signal2 int         null comment '输出信号2次数',
    count_input_signal3 int         null comment '输出信号3次数',
    count_input_signal4 int         null comment '输出信号4次数',
    count_input_signal5 int         null comment '输出信号5次数',
    count_input_signal6 int         null comment '输出信号6次数',
    count_input_signal7 int         null comment '输出信号7次数',
    count_input_signal8 int         null comment '输出信号8次数',
    count_input_signal9 int         null comment '输出信号9次数',
    count_input_signal10 int         null comment '输出信号10次数',
    count_input_signal11 int         null comment '输出信号11次数',
    count_input_signal12 int         null comment '输出信号12次数',
    count_input_signal13 int         null comment '输出信号13次数',
    count_input_signal14 int         null comment '输出信号14次数',
    count_input_signal15 int         null comment '输出信号15次数',
    PRIMARY KEY (vin, day)
);